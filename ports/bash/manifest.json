{
  "name": "Bash",
  "description": "Bash console application running using NativeClient",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    {
      "socket": [
        "tcp-listen:*:*",
        "tcp-connect",
        "resolve-host",
        "udp-bind:*:*",
        "udp-send-to:*:*"
      ]
    }
  ],
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  }
}
