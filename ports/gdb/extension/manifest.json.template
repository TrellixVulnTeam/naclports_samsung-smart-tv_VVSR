{
  "name": "NaCl Debugger",
  "description": "Debugger for Native Client Modules",
  "version": "%(version)s",
  "manifest_version": 2,
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "devtools_page": "devtools.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "externally_connectable": {
    "ids": ["*"],
    "matches": ["*://nacltools.storage.googleapis.com/*"]
  },
  "permissions": [
    "processes"
  ]
}
